<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.moviebrowseapp.presentation">

    <application
        android:theme="@style/AppTheme"
        tools:node="merge">
        <activity android:name=".ui.discover.DiscoverMovieActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.details.DiscoverMovieDetailsActivity" />
    </application>

</manifest>
